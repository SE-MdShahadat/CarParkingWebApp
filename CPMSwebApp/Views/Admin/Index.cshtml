
@using CPMSCommon


<h2>Admin</h2>
<script src="@Url.Content("~/Scripts/jquery-3.4.1.min.js")?@AppSettings.Version"></script>
<div class="content-wrapper">
    <section class="content-header">
        <h1>
            Add New Admin
        </h1>

    </section>
    <section class="content">
        <button id="btnAddNew" class="btn btn-default">Add New</button>
        <button id="btnShowAll" class="btn btn-default">Show All</button>
        <div class="box">
            <div class="box-header with-border" style="display:none;">
                <h3 class="box-title">Title</h3>
                <ul class="box-controls pull-right">
                    <li><a class="box-btn-slide" href="#"></a></li>
                    <li><a class="box-btn-fullscreen" href="#"></a></li>
                </ul>
            </div>
            <div id="tblAdminBody" class="box-body">
                <div id="tblAdmin">
                    @*<table class="table table-bordered table-striped ">
                        <thead>
                            <tr>
                                <th>AdminID</th>
                                <th>Name</th>
                                <th>Contact</th>
                                <th>Address</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td>AdminID</td>
                            </tr>
                        </tbody>
                    </table>*@
                </div>
            </div>
            <div class="box-footer hidden">
                Footer
            </div>
        </div>
    </section>
</div>
<div id="modalAdmin" class="modal fade" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content modal-lg">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Admin Name</h4>
            </div>
            <div id="BuyerCreateForm" class="modal-body modalBodyBack">
                <div class="row">
                    <div class="col-sm-12 col-md-6 col-lg-6">
                        <div class="form-group">
                            <label for="txtName">Name:</label>
                            <input type="text" data-required="1" class="form-control txt" id="txtName" placeholder="Enter Buyer Name.." />
                            <input type="hidden" id="hdAdminID" />
                        </div>
                    </div>
                    <div class="col-sm-12 col-md-6 col-lg-6">
                        <div class="form-group">
                            <label for="txtContact">Contact:</label>
                            <input type="text" class="form-control txt allowdecimal" id="txtContact" placeholder="" />
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-sm-12 col-md-6 col-lg-6">
                        <div class="form-group">
                            <label for="txtAddress">Address:</label>
                            <input type="text" class="form-control txt allowdecimal" id="txtAddress" placeholder="Enter Average CM.." />
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button id="btnSave" type="button" class="btn btn-success">Save</button>
                <button id="btnClear" type="button" class="btn btn-info">Clear</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<script src="@Url.Content("~/CustomContents/Js/Admin.js")?@AppSettings.Version"></script>